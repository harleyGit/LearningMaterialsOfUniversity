<%@ page language="java" pageEncoding="UTF-8"%>
<html>
	<head>
		<title>逻辑运算</title>
	</head>

	<body>
		<h3>
			true && false ? or true and false ?   &nbsp;&nbsp;result： &nbsp;${true&&false}
		</h3>
		<h3>
			true || false ? or true or false ?   &nbsp;&nbsp;result： &nbsp;${true or false}
		</h3>
		<h3>
		!true ? or not true ?   &nbsp;&nbsp;result： &nbsp;${not true}
		</h3>
	</body>
</html>
